@import "../../css/common"

.detail-page
  background-color: #EAEBEA !important

.vpage
  &.page-white
    background-color: #ffffff !important

#main-menu-detail
  position: fixed
  left: 0
  right: 0
  bottom: 0
  height: 3rem
  background-color: #5C5959
  font-size: 0.625rem
  color: #fff
  z-index: 500
  vendor-property: display box
  box-orient: horizontal
  box-align: center
  box-pack: center
  height: 3rem
  .main-menu-item-detail
    vendor-property: display box
    box-pack: center
    width: 25%
    &:active
      color: #bb4a39
    .dbutton
      size: 3.5625rem 1.625rem
      text-align: center
      .icon
        padding-top: 1.25rem
        &.comment
          background: image('icon_menu_pl.png') no-repeat top center
          background-size: 0.75rem 0.75rem
        &.favorite
          background: image('icon_menu_sc.png') no-repeat top  center
          background-size: 1rem 1rem
        &.add
          background: image('icon_menu_gzh.png') no-repeat top center
          background-size: 1.0625rem 0.9375rem
        &.fd
          background: image('icon_menu_fd.png') no-repeat top center
          background-size: 0.8125rem 0.8125rem

.banner-indicate
  position: absolute
  bottom: 0
  right: 0.5rem
  z-index: 500
  ul
    vendor-property: display box
    box-orient: horizontal
    list-style-type: none
    height: 0.625rem
    > li
      size: 0.375rem 0.375rem
      border-radius: 0.375rem
      background: #3E3E3E
      margin: 0 0.1875rem
      &.hightlight
        background: #fff

.common-block
  background: #fff
  &:before
    position: absolute
    content: ''
    left: 0
    right: 0
    height: 1px
    top: -1px
    background: image('line.png') repeat-x left bottom
    background-size: 1px
  &:after
    position: absolute
    content: ''
    left: 0
    right: 0
    height: 1px
    bottom: 0
    background: image('line.png') repeat-x left bottom
    background-size: 1px
  &.nobefore
    &:before
      height: 0px
  &.noafter
    &:after
      height: 0px
  &.mtop
    margin-top: 0.3125rem
  &.nobg
    background: transparent
  .desc-text
    font-size: 0.75rem
    color: #929292
    line-height: 1.25rem
    padding: 0 0.9375rem

.common-header-item
  font-size: 0.875rem
  font-weight: bold
  color: #000
  line-height: 2.25rem
  padding-left: 0.9375rem
  &.nopadding
    padding: 0px;
  > label
    padding-left: 0.9375rem
  .recommendlist
    padding-left: 0
    font-weight: normal
  > span
    color: #777777
  .saler-txt
    font-weight: normal
    margin-left: 0.625rem
    font-size: 0.75rem
    color: #929292
    padding-left: 1rem
    line-height: 2.25rem
    background: image('telbg.png') no-repeat left center
    background-size: 0.8125rem 0.8125rem
  .more
    font-weight: normal
    position: absolute
    top: 0px
    right: 0.9375rem
    font-size: 0.75rem
    color: #bb4a39
    padding-right: 0.5rem
    background: image('arrow4.png') no-repeat right center
    background-size: 0.4375rem 0.75rem
    &:active
      color: #843128
      // background-image: image('arrow4-down.png')
  .comment-detail
    color: #777777
    .stamp
      padding-right: 1rem
      background: image('stamp.png') no-repeat right center
      background-size: 0.9375rem 0.9375rem
    .assist
      padding-right: 1rem
      background: image('assist.png') no-repeat right center
      background-size: 0.9375rem 0.9375rem

.common-cell-item
  vendor-property: display box
  box-orient: horizontal
  box-align: center
  box-pack: center
  &.zxdt
    height: 2.75rem
    line-height: 2.75rem
    padding-left: 0.9375rem
  &:active
    background: rgba(0,0,0,0.1)
    &:before
      background-image: image('arrow-down.png')
  &:after
    position: absolute
    content: ''
    left: 0
    right: 0
    height: 1px
    bottom: 0
    background: image('line.png') repeat-x left bottom
    background-size: 1px
  &:last-child
    &:after
      height: 0
  &:before
    position: absolute
    content: ''
    size: 0.4375rem 0.75rem
    top: 50%
    right: 0.9375rem
    margin-top: -(@height / 2)
    background: image('arrow.png') no-repeat center center
    background-size: 0.4375rem 0.75rem
  > img
    size: 2.5rem 2.5rem
    margin: 0.3125rem 0.625rem 0.3125rem 0.9375rem
  .common-cell-body
    box-flex: 1
    margin-right: 1.875rem
    .common-cell-content
      overflow: hidden
      .common-cell-text
        font-size: 0.75rem
        color: #929192
        line-height: 1.25rem
        > span
          color: #bb4a39

.reply-cell-item
  vendor-property: display box
  box-orient: horizontal
  // box-align: center
  // box-pack: center
  padding: 0.5rem 0.9375rem
  &:after
    position: absolute
    content: ''
    left: 0
    right: 0
    height: 1px
    bottom: 0
    background: image('line.png') repeat-x left bottom
    background-size: 1px
  &:last-child
    &:after
      height: 0
  > img
    size: 2.1875rem 2.1875rem
    border-radius: 2.1875rem
  .reply-cell-body
    box-flex: 1
    margin-left: 0.5rem
    .reply-cell-header
      vendor-property: display box
      box-orient: horizontal
      box-align: center
      box-pack: center
      .name
        font-size: 0.875rem
        color: #000
      .time
        box-flex: 1
        font-size: 0.75rem
        color: #929292
        padding-left: 0.5rem
      .icon
        size: 0.9375rem 0.9375rem
        &.stamp
          margin-right: 0.9375rem
          background: image('stamp.png') no-repeat center center
          background-size: cover
        &.assist
          background: image('assist.png') no-repeat center center
          background-size: cover
    .reply-cell-desc
      font-size: 0.75rem
      color: #929292
      line-height: 1rem
      margin-top: 0.625rem

.saler-common-item
  vendor-property: display box
  box-orient: horizontal
  box-align: center
  box-pack: center
  padding: 0.5rem 0.9375rem
  .saler-common-body
    vendor-property: display box
    box-orient: vertical
    box-align: center
    box-pack: center
    box-flex: 1
    .imgheader
      size: 3.125rem 3.125rem
      vendor-property: display box
      box-orient: vertical
      box-align: center
      box-pack: start
      > img
        display: block
        size: 2.8125rem 2.8125rem
        border-radius: 2.8125rem
      .headbg
        position: absolute
        top: 0
        left: 50%
        margin-left: -1.375rem
        size: 2.8125rem 3.125rem
        background: image('headimgborder.png') no-repeat center center
        background-size: 2.8125rem 3.125rem
    .name
      font-size: 0.75rem
      text-align: center
      color: #000
      line-height: 1.25rem
    .stars
      vendor-property: display box
      box-orient: horizontal
      box-align: center
      box-pack: center
      height: 1.25rem
      .star-hl
        margin: 0 1px
        size: 0.625rem 0.625rem
        background: image('star-3.png') no-repeat center center
        background-size: cover
      .star
        margin: 0 1px
        size: 0.625rem 0.625rem
        background: image('star-1.png') no-repeat center center
        background-size: cover
      .star-dark
        margin: 0 1px
        size: 0.625rem 0.625rem
        background: image('star-2.png') no-repeat center center
        background-size: cover

#page-detail
  padding-bottom: 3rem
  .banner
    height: 8.125rem
    max-height: 8.125rem
    width: 100%
    background: rgba(0,0,0,0.4)
    img
      height: 8.125rem
      max-height: 8.125rem
      display: block
      width: 100%
      margin-top: 0
  .actbar
    height: 2.5rem
    line-height: @height
    vendor-property: display box
    box-orient: horizontal
    box-align: center
    box-pack: center
    background: #bb4a39
    color: #fff
    font-size: 0.75rem
    text-align: center
    .actbtn
      width:  50%
      vendor-property: display box
      box-orient: horizontal
      box-align: center
      box-pack: center
      &:active
        background: #843128
      &.x1
        > div
          background: image('icon_yhtz.png') no-repeat left center
          background-size: 0.625rem 0.625rem
          padding-left: 0.75rem
        &:after
          position: absolute
          content: ''
          right: 0
          top: 0.3125rem
          width: 1px
          height: 1.875rem
          background: image('line2.png') repeat-y
          background-size: 1px
      &.x2
        > div
          background: image('icon_kfyz.png') no-repeat left center
          background-size: 0.4375rem 0.625rem
          padding-left: 0.625rem

  .project-info
    .pro-infomation
      padding: 0.9375rem 0.9375rem 0.625rem 0.9375rem
      .header-info
        vendor-property: display box
        box-orient: horizontal
        box-align: center
        box-pack: center
        .title
          font-size: 1rem
          font-weight: bold
          color: #000
          line-height: 1.875rem
          box-flex: 1
        .favorite
          size: 1.875rem 1.875rem
          background: image('favorite.png') no-repeat right center
          background-size: 1.25rem 1rem
          &:active,
          &.f2
            background: image('favorite-down.png') no-repeat right center
            background-size: 1.25rem 1rem
        .navigation
          size: 1.875rem 1.875rem
          background: image('icon_nav.png') no-repeat right center
          background-size: 1.125rem 1.125rem
      .info-desc
        font-size: 0.75rem
        color: #929192
        line-height: 1.125rem
        > span
          margin-right: 1.875rem
          margin-left: 3.875rem
          margin-top: -1.125rem
          display: block
          min-height: 1.25rem
        .addr
          margin-left: 4.375rem
        .hasmanagerprice
          line-height: 0.875rem
          margin-left: 0.625rem
          font-size: 0.625rem
          // font-weight: bold
          padding: 0.125rem 0.375rem
          text-align: center
          color: #fff
          background-color: #bb4a39
          &:active
            background-color: #843128
          // height: 0.75rem
          // line-height: 0.75rem
          // text-align: center
          // font-size: 0.625rem
          // color: #878888
          // border-image url(../images/border.png) 4 4 4 4 stretch stretch
          // border-width: 0.125rem
          // padding: 0px 0.25rem
          // margin-left: 0.625rem
          // &:active
          //   color: #bb4a39
      .tagheader
        height: 1.5625rem
        vendor-property: display box
        box-orient: horizontal
        .tagborder
          height: 0.75rem
          line-height: 0.75rem
          text-align: center
          font-size: 0.625rem
          color: #878888
          border-image url(../images/border.png) 4 4 4 4 stretch stretch
          border-width: 0.125rem
          padding: 0px 0.25rem
          margin: 0.375rem 0.1875rem
          &.hasactive
            &:active
              color: #bb4a39
        .more
          position: absolute
          bottom: 0.3125rem
          right: 0
          font-size: 0.75rem
          color: #bb4a39
          padding-right: 0.5rem
          background: image('arrow4.png') no-repeat right center
          background-size: 0.4375rem 0.75rem
          &:active
            color: #843128
  .project-zb
    .icons-item
      padding: 0.3125rem 0
      height: 3.75rem
      vendor-property: display box
      box-orient: horizontal
      box-align: center
      box-pack: center
      .item-icon-txt
        box-flex: 1
        color: #929192
        font-size: 0.75rem
        text-align: center
        &.hasafter
          &:after
            position: absolute
            content: ''
            right: 0
            top: 0
            bottom: 0
            width: 1px
            height: 3.5rem
            background: image('line2.png') repeat-y
            background-size: 1px
        &:active
          background: rgba(0,0,0,0.1)
        .icon
          size: 2.5rem 2.5rem
          background-image: image('icons.png')
          background-size: 12.75rem auto
          margin: 0 auto
          &.jj
            background: image('icon_ssjj.png') no-repeat center center
            background-size: 1.5625rem 1.5625rem
          &.ms
            background: image('icon_zbms.png') no-repeat center center
            background-size: 1.5625rem 1.5625rem
          &.gw
            background: image('icon_zbgw.png') no-repeat center center
            background-size: 1.5625rem 1.5625rem
          &.more
            background: image('icon_gdjc.png') no-repeat center center
            background-size: 1.5625rem 1.5625rem
        > div
          &:last-child
            line-height: 1rem
  .project-nav
    .iframe
      height: 9.375rem
      width: 100%
    .nav
      position: absolute
      left: 50%
      margin-left: -2rem
      top: 4.375rem
      size: 4rem 4rem
      background: image('nav.png') no-repeat center center
      background-size: 4rem 4rem
      &:active
        background-image: image('nav-down.png')
  .project-detail
    padding: 0.625rem 0.9375rem
    .detail-item
      vendor-property: display box
      box-orient: horizontal
      label
        width: 3.875rem
        text-align: right
        display: block
        font-size: 0.75rem
      > div
        font-size: 0.75rem
        color: #000
        line-height: 1.125rem
        box-flex: 1
        padding-left: 0.625rem
        color: #929292
  .project-housetype
    .housetype
      height: 11.25rem
      .roomarea
        .room2
          width: 45%
          text-align: center
          line-height: 1.25rem
          color: #929292
          font-size: 0.75rem
          > img
            size: 8.75rem 8.75rem
            margin: 0 auto
        .room
          width: 50%
          text-align: center
          line-height: 1.25rem
          color: #929292
          font-size: 0.75rem
          > img
            size: 9.375rem 9.375rem
            margin: 0 auto
      .next-btn
        position: absolute
        z-index: 1000
        right: 0
        top: 0
        width: 1.875rem
        height: 11.25rem
        background: image('arrow3.png') no-repeat center center
        background-size: 0.5rem 0.8125rem
        &:active
          background-image: image('arrow3-down.png')

#page-forum
  .vpage-content
    .forum-like
      vendor-property display box
      box-orient: horizontal
      box-align: center
      box-pack: center
      // box-pack end
      // .header-text
      //   box-flex: 1
      //   text-align: left
      //   font-size: 0.75rem
      //   color: #929292
      //   padding-left: 0.625rem
      .like-button
        vendor-property display box
        box-pack center
        box-orient vertical
        margin 0.3125rem 1.25rem
        .memo
          font-size 0.75rem
          text-align center
          margin-left -0.25rem
          color #929192
          font-weight 900
        .prompt
          size 1.25rem 1.25rem
          color #929192
          font-size 1.625rem
          position absolute
          right 0.3125rem
          top -0.625rem
          animation fly-up .8s linear 1
          opacity 0
        &:last-child
          margin-right 0.9375rem
        &.like
          .pic
            size 3.8125rem 3.3125rem
            background image('forum-like-pic.png')
            background-size @width @height
          .mark
            size 2rem 1.0625rem
            background image('forum-like-mark.png')
            background-size @width @height
            position absolute
            left 0
            bottom 0px
        &.like
          &.close
            .pic
              size 3.8125rem 3.3125rem
              background image('forum-like-pic-close.png')
              background-size @width @height
            .mark
              size 2rem 1.0625rem
              background image('forum-like-mark-close.png')
              background-size @width @height
              position absolute
              left 0
              bottom 0px
        &.nolike
          .pic
            size 3.375rem 3.375rem
            background image('forum-nolike-pic.png')
            background-size @width @height
          .mark
            size 2.625rem 1.0625rem
            background image('forum-nolike-mark.png')
            background-size @width @height
            position absolute
            left 0
            bottom 0px
        &.nolike
          &.close
            .pic
              size 3.375rem 3.375rem
              background image('forum-nolike-pic-close.png')
              background-size @width @height
            .mark
              size 2rem 1.0625rem
              background image('forum-nolike-mark-close.png')
              background-size @width @height
              position absolute
              left 0
              bottom 0px
    .reply-box
      .header
        vendor-property display box
        .header-img
          size 1.1875rem 1rem
          background image('forum-icon1.png')
          background-size @width @height
          margin-left 1.875rem
        .header-text
          color #929192
          font-size 0.75rem
          line-height 1.5rem
          margin-left 0.3125rem
      .submit-box
        height 6.25rem
        margin-top: 0.625rem
        vendor-property display box
        box-pack center
        box-align center
        background #fff
        .reply
          size 18.75rem 4.375rem
          border none
          border-width 0
          background-color transparent
          padding 0.625rem
          margin 0
          color #929192
          font-size 0.875rem
      .bt-panel
        vendor-property display box
        box-pack end
        .submit
          size 5.125rem 1.25rem
          border-image image('forum-submit.png') 2 2 2 2 stretch stretch
          border-width 1px
          text-align center
          line-height @height
          font-size 0.875rem
          color #F29600
          margin 0.5rem 1.25rem 0px 0px
          &:active
            border-image image('forum-submit-down.png') 2 2 2 2 stretch stretch
            color #000
    .reply-list
      min-width 20rem
      .reply-item
        vendor-property display box
        padding 0.75rem 0px 0.75rem 0px
        &:after
          position: absolute
          content: ''
          left: 0
          right: 0
          height: 1px
          bottom: 0
          background: image('line.png') repeat-x left bottom
          background-size: 1px
        .item-left
          width 3.75rem
          vendor-property display box
          box-pack end
          padding-right 0.625rem
          .user-img
            size 2.25rem 2.25rem
            display block
            border-radius 2.25rem
        .item-center
          box-flex 1
          vendor-property display box
          box-pack center
          box-orient vertical
          padding-right 0.625rem
          .user-name
            font-size 0.875rem
            margin-top 0.125rem
          .reply-date
            color #929192
            font-size 0.75rem
            margin-top 0.25rem
          .reply-content
            color #929192
            font-size 0.875rem
            margin-top 0.25rem
            word-wrap break-word

.backtop
  size 1.875rem 1.875rem
  background image('backtop.png')
  background-size @width @height
  position fixed
  bottom 3.125rem
  right 1.25rem
  opacity 1
  &.in
    animation fade-in 1s linear 1
    opacity 1
  &.out
    animation fade-out 1s linear 1
    opacity 0

