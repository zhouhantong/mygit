@import "../../css/common"

.page-gray
  background-color: #efefef !important
  > section:first-child
    padding-top: 2.5rem

.page-white
  background-color: #fff !important

.page-common
  background-color: #efefef !important

.page-black
  background-color: #000 !important

.page-record
  background-color: #F7F7F7 !important

.sbtn-bar
  vendor-property: display box
  box-orient: horizontal
  box-align: center
  box-pack: center
  margin: 1.25rem 0
  .next-button
    size: 5.3125rem 2rem
    line-height: @height
    color: #fff
    font-size: 0.75rem
    border-radius: 0.25rem
    text-align: center
    &.t1
      background-color: #bb4b38
      &:active
        background-color: #923524
    &.t2
      background-color: #a3ba8b
      &:active
        background-color: #829561
    &.t3
      background-color: #99B1C7
      &:active
        background-color: #B2CBEF
    &.t4
      background-color: #388FA3
      &:active
        background-color: #1382B6
    &.t5
      size: 16.25rem 2.5rem
      line-height: @height
      background-color: #bb4b38
      &:active
        background-color: #923524

.common-info-bar
  vendor-property: display box
  box-orient: horizontal
  box-align: center
  box-pack: start
  height: 2.5rem
  > div
    &:last-child
      border-right: 0px
  .info-block
    width: 20%
    vendor-property: display box
    box-orient: vertical
    box-align: center
    box-pack: center
    height: 2.25rem
    margin-top: 0.25rem
    border-right: 1px dashed #ebebeb
    > div
      line-height: 1rem
      font-size: 0.75rem
      color: #717171
      &:last-child
        color: #F6AC00

.common-topheader
  position: fixed
  z-index: 1000
  top: 0
  left: 0
  right: 0
  height: 2.5rem
  line-height: @height
  text-align: center
  font-size: 0.875rem
  color: #fff
  &.c1
    background-color: #B94C3D
  &.c2
    background-color: #65AEA2
  &.c3
    background-color: #4D998B
  &.c4
    background-color: #388FA3
  &.c5
    background-color: #99B1C7
  &.ct
    position: absolute
    top: -3.125rem

.common-topheader2
  height: 2.5rem
  line-height: @height
  text-align: center
  font-size: 0.875rem
  color: #fff
  &.c1
    background-color: #B94C3D

.form-table-view
  .input-area
    vendor-property: display box
    box-orient: horizontal
    box-align: center
    font-size: 0.875rem
    height: 2.75rem
    &:after
      position: absolute
      content: ''
      left: 4.0625rem
      right: 0
      height: 1px
      bottom: 0
      background: image('line.png') repeat-x left bottom
      background-size: 1px
    &.hasactive
      &:active
        background-color: rgba(0,0,0,0.1)
    > label
      display: block
      width: 4.0625rem
      text-align: center
      font-size: 0.75rem
      height: 2.75rem
      line-height: @height
      position: relative
      &:after
        position: absolute
        content: ""
        size: 0.375rem 0.5rem
        right: 0
        top: 50%
        margin-top: -(@height / 2)
        background: image('arrow2.png') no-repeat center center
        background-size: cover
    .noafter
      &:after
        background: none
    .text-value
      &:after
        position: absolute
        content: ""
        size: 0.5rem 0.625rem
        right: 0.3125rem
        top: 50%
        margin-top: -(@height / 2)
        margin-right: -0.125rem
        background: image('arrow.png') no-repeat center center
        background-size: contain
    > input,
    .text-value
      box-flex: 1
      display: block
      border: none
      font-size: 0.75rem
      text-align: right
      background-color: transparent
      height: 1.875rem
      padding: 0px
      margin: 0 0.3125rem
      line-height: @height
      -webkit-appearance: inherit
    .text-value
      padding-right: 0.9375rem
    > select
      position: absolute
      left: 0
      right: 0
      top: 0
      bottom: 0
      opacity: 0
      z-index: 1000
      width: 100%
  .button-vericode
    vendor-property: display box
    box-orient: horizontal
    box-align: center
    box-pack: center
    height: 2.75rem
    line-height: 1rem
    background: #E1E2E2
    font-size: 0.75rem
    color: #929292
    width: 4.375rem
    text-align: center
    &.disabled
      color: #fff
      background: #929292
      &:active
        color: #fff
        background: #929292
    &:active
      color: #fff
      background-color: #929292

.form-input-view
  margin: 0.625rem
  .input-area
    vendor-property: display box
    box-orient: horizontal
    box-align: center
    font-size: 0.875rem
    padding: 0 0.5rem
    margin-top: 0.625rem
    height: 2.5rem
    line-height: @height
    -webkit-appearance: inherit
    border-width: 1px
    border-image: image('border.png') 2 2 2 2 stretch stretch
    &.need-input
      > label
        &:before
          position: absolute !important
          content: "*"
          size: 0.375rem 0.375rem
          color: #FB0006
          left: -0.1875rem
          top: 0.1875rem
    .chgbtn
      width: 2.5rem
      font-size: 1.25rem
      text-align: center
      color: #929292
      margin-right: -0.5rem
      &:active
        background-color: #EBEBEB
      &:before
        position: absolute
        content: ''
        left: 0
        top: 0
        bottom: 0px
        width: 1px
        height: 2.5rem
        background: image('line2.png') repeat-y
        background-size: 1px 1px
    > label
      display: block
      width: 3.75rem
      text-align: center
      font-size: 0.75rem
      position: relative
      &:after
        position: absolute
        content: ''
        right: -0.5rem
        top: 0.3125rem
        bottom: 0px
        width: 1px
        height: 1.875rem
        background: image('line2.png') repeat-y
        background-size: 1px 1px
    .text-value
      &:after
        position: absolute
        content: ""
        size: 0.5rem 0.625rem
        right: 0
        top: 50%
        margin-top: -(@height / 2)
        background: image('arrow.png') no-repeat center center
        background-size: contain
    > input,
    .text-value
      box-flex: 1
      display: block
      border: none
      font-size: 0.75rem
      text-align: right
      background-color: transparent
      height: 1.875rem
      padding: 0px
      margin: 0 0.3125rem
      line-height: @height
      -webkit-appearance: inherit
    .text-value
      padding-right: 0.9375rem
    > select
      position: absolute
      left: 0
      right: 0
      top: 0
      bottom: 0
      opacity: 0
      z-index: 1000
      width: 100%


